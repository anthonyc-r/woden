let WMReversibleAffineTransform := Structure <<< ('WMReversibleAffineTransform' , typeNameSuffix) asSymbol
	slots: {
		#matrix => WMMatrix3. 
		#translation => WMVector4. 
		#inverseMatrix => WMMatrix3. 
		#inverseTranslation => WMVector4.
	};
	package: 'WodenMath-Core-Transforms';
	yourself.

Compiler
	evaluateFileNamedInThisContext: 'WMReversibleAffineTransform.ltk';
	yourself.
